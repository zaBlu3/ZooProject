
@{

if (TempData["ErrorMessage"] != null)
{
   
	<script type="text/javascript">
		toastr.error('@TempData["ErrorMessage"]', 'Error!', { timeOut : 1500});
	</script>
}		
else if (TempData["SuccessMessage"] != null)
{
  	<script type="text/javascript">
			toastr.success(`@TempData["SuccessMessage"]`, 'Success!', { timeOut: 1500 });
	</script>
}

}
